<h1 style="margin-top:3em">Sign In</h1>

<%= simple_form_for :session, { url: session_path } do |f| %>

  <%= f.input :email %>

  <%= f.input :password %>

  <%= f.submit 'Sign In', class: 'btn btn-outline-primary'  %>

  <%= link_to 'Sign Up', new_user_path, class: 'btn btn-outline-primary' %>

<% end %>
